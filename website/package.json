{
  "name": "website",
  "version": "0.0.0",
  "scripts": {
    "app:start": "NODE_OPTIONS=--preserve-symlinks ng serve",
    "app:build": "NODE_OPTIONS=--preserve-symlinks ng build --configuration production",
    "app:build:dev": "NODE_OPTIONS=--preserve-symlinks ng build --watch --configuration development",
    "app:watch": "NODE_OPTIONS=--preserve-symlinks ng build --watch --configuration development",
    "app:test": "NODE_OPTIONS=--preserve-symlinks ng test",
    "ssr:build": "NODE_OPTIONS=--preserve-symlinks ng run app:server",
    "ssr:watch": "NODE_OPTIONS=--preserve-symlinks ng run app:server:development --watch",
    "server": "NODE_OPTIONS=--preserve-symlinks TS_NODE_PROJECT=tsconfig.server.json node -r ts-node/register -r tsconfig-paths/register src/server/app.ts server:start",
    "server:prod": "TS_NODE_BASEURL=dist node -r tsconfig-paths/register dist/src/server/app.js server:start",
    "server:build": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "server:watch": "NODE_OPTIONS=--preserve-symlinks TS_NODE_PROJECT=tsconfig.server.json node -r ts-node/register -r tsconfig-paths/register --watch --watch-path dist/app/server --watch-path src/ src/server/app.ts server:start",
    "dev:frontend": "rm -rf .angular && concurrently --kill-others \"npm run app:watch\" \"npm run ssr:watch\"",
    "dev": "rm -rf .angular && concurrently --kill-others \"npm run app:watch\" \"npm run ssr:watch\" \"npm run server:watch\"",
    "app": "NODE_OPTIONS=--preserve-symlinks TS_NODE_PROJECT=tsconfig.server.json node -r ts-node/register -r tsconfig-paths/register src/server/app.ts",
    "build": "npm run app:build && npm run ssr:build && npm run server:build",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "private": true,
  "dependencies": {
    "@angular/ssr": "^17.1.0",
    "@deepkit/app": "^1.0.1-alpha.102",
    "@deepkit/core": "^1.0.1-alpha.100",
    "@deepkit/desktop-ui": "^1.0.1-alpha.102",
    "@deepkit/event": "^1.0.1-alpha.102",
    "@deepkit/framework": "^1.0.1-alpha.103",
    "@deepkit/http": "^1.0.1-alpha.102",
    "@deepkit/injector": "^1.0.1-alpha.102",
    "@deepkit/logger": "^1.0.1-alpha.102",
    "@deepkit/mongo": "^1.0.1-alpha.103",
    "@deepkit/orm": "^1.0.1-alpha.102",
    "@deepkit/postgres": "^1.0.1-alpha.103",
    "@deepkit/sql": "^1.0.1-alpha.103",
    "@deepkit/type": "^1.0.1-alpha.102",
    "angular-plotly.js": "^5.2.2",
    "discord.js": "^14.13.0",
    "gray-matter": "^4.0.3",
    "marked": "^9.0.0",
    "ngx-markdown": "^17.1.1",
    "openai": "^4.8.0",
    "plotly.js-dist-min": "^2.27.0",
    "pybridge": "^1.1.0",
    "rehype-raw": "^4.0.0",
    "rehype-react": "^5.0.0",
    "rehype-slug": "^3.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "rxjs": "~7.8.0",
    "rxjs-for-await": "^1.0.0",
    "tiny-glob": "^0.2.9",
    "unified": "^11.0.3",
    "unist-util-visit": "2.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.0",
    "@angular/animations": "^17.1.0",
    "@angular/cdk": "^17.1.0",
    "@angular/cli": "~17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/compiler-cli": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/platform-server": "^17.1.0",
    "@angular/router": "^17.1.0",
    "@deepkit/type-compiler": "^1.0.1-alpha.102",
    "@types/jest": "^29.5.5",
    "@types/prismjs": "^1.26.0",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "prismjs": "^1.29.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.3.0",
    "typescript": "~5.3.3",
    "zone.js": "~0.14.3"
  }
}
