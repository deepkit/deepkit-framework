{
  "name": "@marcj/benchmark",
  "version": "2.1.4",
  "description": "Benchmark stuff.",
  "sideEffects": false,
  "private": true,
  "scripts": {
    "test": "jest --coverage",
    "tsc": ""
  },
  "repository": "https://github.com/marcj/marshal",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
    "@marcj/marshal": "^2.1.4",
    "ajv": "^6.11.0",
    "buffer": "^5.2.1",
    "cerialize": "^2.0.1",
    "class-transformer": "^0.2.0",
    "quartet": "^9.1.13"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/node_modules/**",
      "!**/lib/**/*",
      "!**/*.d.ts",
      "!index.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
