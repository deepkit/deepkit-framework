{
  "name": "@deepkit/bson",
  "version": "1.0.1-alpha.40",
  "description": "Deepkit BSON parser",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "peerDependencies": {
    "@deepkit/core": "^1.0.1-alpha.13",
    "@deepkit/crypto": "^1.0.1-alpha.13",
    "@deepkit/type": "^1.0.1-alpha.13",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@deepkit/core": "^1.0.1-alpha.39",
    "@deepkit/crypto": "^1.0.1-alpha.35",
    "@deepkit/type": "^1.0.1-alpha.40",
    "@types/bson": "^4.0.3",
    "@types/node": "14.14.28",
    "bson": "^4.4.0",
    "bson-ext": "^4.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  },
  "gitHead": "56081823b559bb68b77a8781957af5d9c2e019a7"
}
