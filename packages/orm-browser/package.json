{
  "name": "@deepkit/orm-browser",
  "version": "1.0.7",
  "description": "ORM Browser",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "peerDependencies": {
    "@deepkit/app": "^1.0.1",
    "@deepkit/broker": "^1.0.1",
    "@deepkit/bson": "^1.0.1",
    "@deepkit/core": "^1.0.1",
    "@deepkit/framework": "^1.0.1",
    "@deepkit/http": "^1.0.1",
    "@deepkit/injector": "^1.0.1",
    "@deepkit/logger": "^1.0.1",
    "@deepkit/orm": "^1.0.1",
    "@deepkit/rpc": "^1.0.1",
    "@deepkit/type": "^1.0.1"
  },
  "dependencies": {
    "@deepkit/orm-browser-gui": "^1.0.7"
  },
  "devDependencies": {
    "@deepkit/app": "^1.0.5",
    "@deepkit/broker": "^1.0.7",
    "@deepkit/bson": "^1.0.7",
    "@deepkit/core": "^1.0.5",
    "@deepkit/framework": "^1.0.7",
    "@deepkit/http": "^1.0.5",
    "@deepkit/injector": "^1.0.5",
    "@deepkit/logger": "^1.0.5",
    "@deepkit/orm": "^1.0.5",
    "@deepkit/rpc": "^1.0.7",
    "@deepkit/type": "^1.0.5",
    "@types/node": "^22.13.1",
    "rxjs": "~7.8.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  }
}
