{
  "name": "@deepkit/app",
  "version": "1.0.2",
  "description": "Deepkit App, CLI framework and service container",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "peerDependencies": {
    "@deepkit/core": "^1.0.1",
    "@deepkit/event": "^1.0.1",
    "@deepkit/injector": "^1.0.1",
    "@deepkit/logger": "^1.0.1",
    "@deepkit/stopwatch": "^1.0.1",
    "@deepkit/type": "^1.0.1",
    "@deepkit/workflow": "^1.0.1"
  },
  "devDependencies": {
    "@deepkit/core": "^1.0.1",
    "@deepkit/event": "^1.0.2",
    "@deepkit/injector": "^1.0.2",
    "@deepkit/logger": "^1.0.2",
    "@deepkit/stopwatch": "^1.0.2",
    "@deepkit/type": "^1.0.2",
    "@deepkit/workflow": "^1.0.2",
    "conditional-type-checks": "^1.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/tsconfig.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  },
  "gitHead": "56081823b559bb68b77a8781957af5d9c2e019a7"
}
