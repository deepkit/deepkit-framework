  "devDependencies": {
    "@deepkit/core": "^1.0.1-alpha.31",
    "@deepkit/orm": "^1.0.1-alpha.33",
    "@deepkit/orm-integration": "^1.0.1-alpha.13",
    "@deepkit/sql": "^1.0.1-alpha.33",
    "@deepkit/type": "^1.0.1-alpha.33",
    "reflect-metadata": "^0.1.13"
