{
  "name": "@deepkit/desktop-ui",
  "version": "1.0.14",
  "description": "Library for desktop UI widgets in Angular 10+",
  "keywords": [
    "Desktop",
    "UI",
    "MacOS"
  ],
  "bin": {
    "dui-create-font": "./bin/create-font.js"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "sideEffects": false,
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    "./src/scss/all.scss": {
      "default": "./src/scss/all.scss"
    },
    "./src/scss/icon.scss": {
      "default": "./src/scss/icon.scss"
    },
    "./src/scss/reset.scss": {
      "default": "./src/scss/reset.scss"
    },
    "./docs.json": {
      "default": "./src/assets/docs.json"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/fesm2022/deepkit-desktop-ui.mjs"
    }
  },
  "scripts": {
    "font": "node bin/create-font.js",
    "build": "npm run font && npm run docjson && rm -rf .angular && ng-packagr --config tsconfig.prod.json && rm dist/.npmignore",
    "watch": "ng-packagr --config tsconfig.prod.json --watch",
    "docjson": "rm -f docs.json && typedoc --json docs.json --options ../../typedoc.js --plugin ../../typedoc.decorators.js src/index.ts && cp docs.json src/assets/docs.json",
    "prepublishOnly": "webpack && node bin/create-font.js"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "on-change": "^4.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^20.0.3",
    "@angular/animations": "^20.0.4",
    "@angular/cdk": "^20.0.3",
    "@angular/cli": "^20.0.3",
    "@angular/common": "^20.0.4",
    "@angular/compiler": "^20.0.4",
    "@angular/compiler-cli": "^20.0.4",
    "@angular/core": "^20.0.4",
    "@angular/forms": "20.0.4",
    "@angular/platform-browser": "^20.0.4",
    "@angular/platform-browser-dynamic": "^20.0.4",
    "@angular/router": "^20.0.4",
    "@deepkit/core": "^1.0.1",
    "@deepkit/core-rxjs": "^1.0.1",
    "@deepkit/event": "^1.0.1",
    "@deepkit/type": "^1.0.1"
  },
  "devDependencies": {
    "@deepkit/core": "^1.0.5",
    "@deepkit/core-rxjs": "^1.0.9",
    "@deepkit/event": "^1.0.12",
    "@deepkit/type": "^1.0.12",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^22.13.1",
    "fs-extra": "^8.1.0",
    "ng-packagr": "^20.0.1",
    "rxjs": "~7.8.0",
    "svg2ttf": "^4.2.0",
    "svgicons2svgfont": "^9.0.4",
    "ts-loader": "^9.4.2",
    "ttf2woff": "^2.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "gitHead": "c1d3a8fba42c89448ee51a0fa42d1b7281edd86c"
}
