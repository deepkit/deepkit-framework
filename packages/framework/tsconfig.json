{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "preserveSymlinks": true,
    "target": "es2018",
    "module": "CommonJS",
    "esModuleInterop": true,
    "outDir": "./dist/cjs",
    "declaration": true,
    "composite": true,
    "jsx": "react-jsx",
    "jsxImportSource": ".."
  },
  "reflection": true,
  "include": [
    "src",
    "jsx",
    "tests",
    "benchmark",
    "index.ts",
    "jsx-runtime.ts",
    "loader.ts"
  ],
