{
  "name": "@deepkit/type-compiler",
  "version": "1.0.1-alpha.97",
  "description": "Deepkit/type compiler/Typescript transformer",
  "types": "./src/index.d.ts",
  "main": "./index.cjs",
  "module": "./index.js",
  "bin": {
    "deepkit-type-install": "./install-transformer.cjs.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "install": "rm -rf node_modules/typescript && node ./dist/install-transformer.js || exit 0"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "peerDependencies": {
    "typescript": "^4.8.2 || ^5.0.0"
  },
  "dependencies": {
    "@deepkit/type-spec": "^1.0.1-alpha.93",
    "@marcj/ts-clone-node": "^2.0.0",
    "@typescript/vfs": "^1.4.0",
    "get-tsconfig": "^4.5.0",
    "lz-string": "^1.4.4",
    "micromatch": "^4.0.5",
    "strip-json-comments": "^3.1.1"
  },
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "default": "./index.cjs",
      "module": "./index.js",
      "import": "./index.js"
    },
    "./install-transformer": {
      "default": "./install-transformer.cjs",
      "module": "./install-transformer.js",
      "import": "./install-transformer.js"
    }
  },
  "reflection": true
}