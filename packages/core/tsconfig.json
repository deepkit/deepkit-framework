{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "target": "es2021",
    "module": "CommonJS",
    "esModuleInterop": true,
    "baseUrl": ".",
    "outDir": "./dist/cjs",
    "declaration": true,
    "composite": true,
    "types": [
      "dot-prop"
    ]
  },
  "reflection": true,
  "include": [
    "src",
    "index.ts",
    "tests"
  ],
  "references": []
}
