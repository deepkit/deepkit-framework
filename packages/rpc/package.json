  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ],
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.test.json"
