  },
  "peerDependencies": {
    "@deepkit/bson": "^1.0.1-alpha.13",
    "@deepkit/core": "^1.0.1-alpha.13",
    "@deepkit/core-rxjs": "^1.0.1-alpha.13",
    "@deepkit/injector": "^1.0.1-alpha.13",
