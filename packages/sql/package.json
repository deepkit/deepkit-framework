{
  "name": "@deepkit/sql",
  "version": "1.0.1-alpha.121",
  "description": "deepkit/orm base SQL",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "require": "./dist/cjs/index.js",
      "browser": "./dist/esm/browser.js",
      "default": "./dist/esm/index.js"
    },
    "./commands": {
      "types": "./dist/cjs/commands.d.ts",
      "require": "./dist/cjs/commands.js",
      "default": "./dist/esm/commands.js"
    }
  },
  "bin": {
    "deepkit-sql": "./bin/deepkit-sql.js"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "echo '{\"type\": \"module\"}' > ./dist/esm/package.json"
  },
  "repository": "https://github.com/deepkit/deepkit-framework",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "peerDependencies": {
    "@deepkit/app": "^1.0.1-alpha.13",
    "@deepkit/core": "^1.0.1-alpha.13",
    "@deepkit/injector": "^1.0.1-alpha.13",
    "@deepkit/logger": "^1.0.1-alpha.13",
    "@deepkit/orm": "^1.0.1-alpha.13",
    "@deepkit/stopwatch": "^1.0.1-alpha.13",
    "@deepkit/type": "^1.0.1-alpha.13"
  },
  "dependencies": {
    "@types/sqlstring": "^2.2.1",
    "date-fns": "^2.17.0",
    "fast-glob": "^3.2.5",
    "sqlstring": "^2.3.2",
    "sqlstring-sqlite": "^0.1.1"
  },
  "devDependencies": {
    "@deepkit/app": "^1.0.1-alpha.121",
    "@deepkit/core": "^1.0.1-alpha.121",
    "@deepkit/injector": "^1.0.1-alpha.121",
    "@deepkit/logger": "^1.0.1-alpha.121",
    "@deepkit/orm": "^1.0.1-alpha.121",
    "@deepkit/stopwatch": "^1.0.1-alpha.121",
    "@deepkit/type": "^1.0.1-alpha.121"
  },
  "jest": {
    "runner": "../../jest-serial-runner.js",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "<rootDir>/tsconfig.spec.json"
        }
      ]
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "testTimeout": 50000,
    "testMatch": [
      "**/tests/**/*.spec.ts"
    ]
  },
  "gitHead": "56081823b559bb68b77a8781957af5d9c2e019a7"
}
